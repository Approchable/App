{
  "expo": {
    "name": "ApproachableNative",
    "userInterfaceStyle": "automatic",
    "slug": "ApproachableNative",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "version": "1.0.6git ",
    "orientation": "portrait",
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "icon": "./assets/appstore.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.approachable.approachablenative",
      "buildNumber": "1.0.6",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.724006010963-er1i8nf3hsoeibm6vg8m01au6gqq8bhb"
        }
      },
        "infoPlist": {
          "NSLocationWhenInUseUsageDescription": "ApproachableNative uses your location to show you nearby places and events."
        }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
